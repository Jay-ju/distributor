.content{
width:100%;
}
.inner{
width:100%;
overflow: hidden;
}
.inner li{
width:100%;
height:16.48rem;/* 412px */
margin-top:0.96rem;
overflow: hidden;
}
.inner a{
width:100%;
height:100%;
display: block;
}
.i_pic{
width:3.84rem;/* 96px */
height:3.84rem;
background: url('../images/index/mo.png') no-repeat center center;
background-size:cover;
border-radius: 50%;
margin:0 0.6rem 0 0.5rem;
float: left;
}
.i_txt{
float: left;
width:75%;
height:100%;
}
.i_time{
width: 100%;
overflow: hidden;
color:#DB304A;
font-weight: normal;
padding-right:0.5rem;
box-sizing:border-box;
}
.i_time em{
font-size: 0.72rem;
float: right;
color:#8A8A8A;
}
.news{
width:97%;
margin-right:3%;
height:15.2rem;
background: #f0f0f0;
position:relative;
padding-left: 5px;
box-sizing:border-box;
}
.news:before{
content:'';
width:0.8rem;
height:0.8rem;
display: block;
background: url('../images/index/sanjiao.png') center center;
background-size:cover;
position: absolute;
left:-0.7rem;
top:0.2rem;
}
.news h2,.news .large{
white-space:nowrap;
word-break:keep-all;
overflow:hidden;
text-overflow:ellipsis;
padding-top: 0.3rem;
}
.news .large{
color:#8F8F8F;
}
.news .larger{
width:100%;
height:2.2m;
text-overflow: -o-ellipsis-lastline;
overflow: hidden;
text-overflow: ellipsis;
display: -webkit-box;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical;
margin-top:0.3rem;
}
.t_img{
width:100%;
height:4.8rem;
margin-top:1.2rem;
}
.t_img img{
width:4.6rem;/* 115px */
height:4.8rem;
}

.tle{
width:100%;
margin:1.44rem 0 0 0;
}
.c_tle{
padding:0 1rem 0 1.6rem;
box-sizing:border-box;
}
.con h3{
margin-top:0.6rem;
}
.con .text-low2{
font-weight: normal;
float: right;
font-size: 0.72rem;
}
.con ul{
margin:1.62rem 0 0 0 ;
}
.con li{
color:#8c8c8c;
padding:0 0.8rem 0 2.1rem ;
margin:0.8rem 0 0 0;
box-sizing:border-box;
white-space:nowrap;
word-break:keep-all;
overflow:hidden;
text-overflow:ellipsis;
}
.con p{
margin-top:1.2rem;
word-wrap:break-word;
overflow:hidden;
}
.con ins{
display: block;
margin:5px auto;
text-align: center;
padding:0 0.8rem;
box-sizing:border-box;
}
/* 晒单详情结束-------------------------------------------- */
.goods{
width:100%;
padding:1.44rem 0 0.8rem;
border-bottom:1px dashed #E7E7E6;
}
.goods_L{
width:33%;
height:5.6rem;
margin:0 0.6rem 0 0.8rem;
float: left;
}
.goods_R{
width:58%;
float: left;
}
.goods_R .text-normal{
    word-wrap:break-word;
    overflow:hidden;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;

}
.goods_R span{
width:100%;
height:1.84rem;
display: block;
margin-top:10px;
}
.goods_R span a{
width:4.6rem;
height:100%;
text-align: center;
line-height: 1.84rem;
display: block;
color:#fff;
border-radius: 6px;
}
.head{
width:100%;
margin-top:0.8rem;
border-top: 1px solid #E7E7E6;
}
.head_t{
width:100%;
height:2rem;
}
.head_t b{
width:5rem;
height:100%;
display:block;
float: right;
font-weight: normal;
background: #FFAB33;
color:#fff;
line-height: 2rem;
text-align: center;
}
.head_c{
width:100%;
height:auto;
padding-bottom: 1rem;
border-bottom:1px dashed #E7E7E6;
}
.head_c p b{
float: right;
font-weight: normal;
}
.head .goods{
border-bottom:1px solid #E7E7E6;
}
.goods_a{
    width:100%;
    height:100%;
    display:block;
}
.footer{
text-align: center;
line-height: 3rem;
}
.Null{
    text-align: center;
    width:100%;
    height:auto;
    padding-bottom:2rem;
}
.Null img{
    margin:2.5rem 0 1rem 0;
}
.b_g {
border: 1px solid #EF3B3B;
background: #fff;
width: 90%;
height: 3.2rem;
display: block;
margin: 1rem auto 0 auto;
text-align: center;
line-height: 3.2rem;
font-size: 16px;
color: #EF3B3B;
border-radius: 6px;
font-weight: 700;
}
/* 发布晒单 ----*/
.txt_t{
width:100%;
height:auto;
}
.txt_tle{
width:100%;
height: auto;
padding:1rem 0.8rem;
}
.txt_tle input{
width:100%;
height:1.5rem;
display: block;
background: none;
border:none;
outline:none;
font-size:1rem;
}
.txt_cen{
width: auto;
height: auto;
margin: 0 auto;
overflow: hidden;
position: relative;
}
#shadow, #text {
width:100%;
font-size: 1.12rem;
overflow-x: hidden; 
height: 3.84rem ;
background: none;
outline:none;
color:#666;
}
#shadow {
position: absolute;
top: 0;
border-width: 0px;
padding: 0px;
visibility: hidden;
}
#text { 
resize: none; 
}
.txt_img{
overflow: hidden;
}
.input li{
width:5.2rem;
height:5.2rem;
display: block;
overflow: hidden;
position: relative;
float: left;
margin-left:10px;
margin-top:1rem;
}
.input li ins{
    position:absolute;
    top:0px;
    right:2px;
    width:20px;
    height:20px;
    display:block;
    background:url('../images/iconfont-shanchu.png') no-repeat center center;
    background-size:cover;
}
.input li img{
width:100%;
height:100%;
}
.input li input{
position: absolute;
top:0;
left:0;
width:100%;
height:100%;
display: block;
opacity: 0;
}
.input .listyle{
background: url('../images/add.png') no-repeat center center;
background-size:100%;
}
.new{
width:95%;
height:auto;
background: #f7f7f7;
margin:10px auto;
font-size: 0.96rem;
color:#666;
}
.new p{
margin-bottom: 1.2rem;
}
.foot{
width:100%;
height:3.6rem;
line-height: 3.6rem;
position: fixed;
bottom:0;
}
.foot a{
width:95%;
height:100%;
display: block;
margin:0 auto;
color:#fff;
text-align: center;
border-radius: 6px;
}
.img_text p{
    width:100%;
    height:auto;
    margin-bottom:-10px;
    text-align: center;
}