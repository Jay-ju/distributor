.number{
width:100%;
height:auto;
margin:1rem auto 2rem auto;
}
.number_in{
	width:100%;
	height:auto;
	background:#F0F0F0;
	padding:10px 0.8rem;
}
.number ul{
	width:94%;
	height:100%;
	display:block;
	background:#fff;
	margin:0 auto;
}
.number li{
float: left;
width:25%;
margin-top:5px;
font-size:0.8rem;
text-align:center;
}
.num_time{
width:100%;
height:4rem;
margin:0 auto;
}
.num_time dl{
width:50%;
height:4rem;
display:block;
float:left;
font-size:14px;
}
.num_time dt,.num_time dd{
height:50%;
border-bottom:1px solid #ccc;
padding:0 0.8rem;
box-sizing:border-box;
line-height:2rem;
}
.num_time dl:nth-of-type(2){
text-align:right;
}
.img{
width:100%;
height:25rem;
background: url('../images/404.png') no-repeat center center;
background-size:80% 100%;
}
.btn{
width:100%;
height:3.68rem;
margin-top:2rem;
}
.btn button{
width:40%;
height:100%;
display: block;
margin:0 auto;
background: none;
border:1px solid #DCDEE0;
outline:none;
}